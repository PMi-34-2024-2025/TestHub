<UserControl x:Class="PE.DesktopApplication.TestHub.WPF.AnswerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PE.DesktopApplication.TestHub.WPF"
             mc:Ignorable="d" Margin="20 5" Width="900">
    <Grid Background="#FF2E44F9">
        <StackPanel Orientation="Horizontal" Margin="5,5,5,5">
            <CheckBox Name="chkIsCorrect" Content="" Margin="2 2"/>
            <TextBox Name="txtAnswer" Width="870" Margin="0,0,10,0" FontSize="14" />
        </StackPanel>
    </Grid>
</UserControl>
